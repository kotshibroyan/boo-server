{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "plugin:prettier/recommended"],
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
        "quotes": ["error", "double"],
        "no-trailing-spaces": "error"
      },
    "plugins": ["prettier"]
  }
  